from collections import Counter
class Solution:
    def firstMissingPositive(self, nums: List[int]) -> int:
        nums.sort()
        i=0
        result = 1
        positives= Counter(list(filter(lambda x: x>0, nums))).keys()
        if (len(positives)>0) and (min(positives) == 1):
            if len(positives) == max(positives):
                result = max(positives)+1
            else:
                j= min(positives)
                while True:
                    j+=1 
                    if j not in positives:
                        result = j
                        break
        return result
